{
  "name": "react-selectable",
  "version": "4.0.0",
  "description": "Enable other React components to be selectable by drawing a box with your mouse/touch",
  "main": "dist/react-selectable.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/Siemko/react-selectable.git"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "dev": "NODE_ENV=development webpack",
    "example": "webpack --config webpack.config.example.js",
    "watch": "NODE_ENV=development webpack --watch",
    "prepare": "npm run build"
  },
  "keywords": [
    "selectable",
    "selection",
    "mouse",
    "drag",
    "react"
  ],
  "author": "Uncle Cheese",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Siemko/react-selectable/issues"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "lodash.throttle": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "babel-loader": "^9.1.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
